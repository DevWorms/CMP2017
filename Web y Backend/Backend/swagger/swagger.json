{
    "swagger": "2.0",
    "info": {
        "description": "API para CMP",
        "version": "0.0.1",
        "title": "CMP WebService"
    },
    "host": "localhost:8000",
    "basePath": "/api",
    "schemes": [
        "http"
    ],
    "paths": {
        "/user/login": {
            "post": {
                "tags": [
                    "user"
                ],
                "summary": "Login de usuario",
                "description": "Login de usuario devuelve el api_key del usuario",
                "operationId": "loginUser",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "user",
                        "in": "formData",
                        "description": "Email del usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "Contraseña del usuario",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid username/password supplied"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/user/signup": {
            "post": {
                "tags": [
                    "user"
                ],
                "summary": "Registro de usuario",
                "description": "Registro de usuario",
                "operationId": "signupUser",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "name",
                        "in": "formData",
                        "description": "Nombre(s)",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "last_name",
                        "in": "formData",
                        "description": "Apellido(s)",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "email",
                        "in": "formData",
                        "description": "Email, no puede repetirse en la BD",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "Contraseña (mínimo 6 caracteres)",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password_confirmation",
                        "in": "formData",
                        "description": "Confirmación de la contraseña",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "type",
                        "in": "formData",
                        "description": "Tipo de usuario (es un string por el momento) Ej. \"Trabajador de Pemex\"",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "association",
                        "in": "formData",
                        "description": "Asociación (es un string por el momento) Ej. \"Asociación 4\"",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid username/password supplied"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/user/profile/{id}/{api_key}": {
            "get": {
                "tags": [
                    "user"
                ],
                "summary": "Perfil de usuario",
                "description": "Devuelve el perfil del usuario",
                "operationId": "userProfile",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Id de usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "api_key",
                        "in": "api_key",
                        "description": "Api Key de usuario",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid username/password supplied"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/programa/create": {
            "post": {
                "tags": [
                    "programa"
                ],
                "summary": "Crea un nuevo programa",
                "description": "Crea un nuevo programa, devuelve el programa",
                "operationId": "newPrograma",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "formData",
                        "description": "Id de usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "api_key",
                        "in": "formData",
                        "description": "Api Key del usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "categoria_id",
                        "in": "formData",
                        "description": "Categoría a la que pertenecerá el evento",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "lugar",
                        "in": "formData",
                        "description": "Lugar donde se llevara a cabo el evento",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "recomendaciones",
                        "in": "formData",
                        "description": "Recomendaciones del evento",
                        "required": true,
                        "type": "text"
                    },
                    {
                        "name": "fecha",
                        "in": "formData",
                        "description": "Fecha del evento. Ej: 2016-03-20",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "latitude",
                        "in": "formData",
                        "description": "Coordenadas del mapa latitude",
                        "required": false,
                        "type": "string"
                    },
                    {
                        "name": "longitude",
                        "in": "formData",
                        "description": "Coordenadas del mapa longitude",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid username/password supplied"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/programa/all/{user_id}/{api_key}": {
            "get": {
                "tags": [
                    "programa"
                ],
                "summary": "Devuelve los últimos 10 eventos creados",
                "description": "Devuelve los últimos 10 eventos creados",
                "operationId": "programaAll",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "description": "Id de usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "api_key",
                        "in": "path",
                        "description": "Api Key del usuario",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid username/password supplied"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/categoria/create": {
            "post": {
                "tags": [
                    "categoria"
                ],
                "summary": "Crear nueva categoría",
                "description": "Crear nueva categoría devuelve la categoría creada",
                "operationId": "categoriaNew",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "formData",
                        "description": "Id de usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "api_key",
                        "in": "formData",
                        "description": "Api Key del usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "nombre",
                        "in": "formData",
                        "description": "Nombre de la categoría",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid username/password supplied"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/categoria/all/{user_id}/{api_key}": {
            "get": {
                "tags": [
                    "categoria"
                ],
                "summary": "Devuelve todas las categorías",
                "description": "Devuelve todas las categorías",
                "operationId": "categoriaAll",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "user_id",
                        "in": "path",
                        "description": "Id de usuario",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "api_key",
                        "in": "path",
                        "description": "Api Key del usuario",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Invalid username/password supplied"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        }
    },
    "definitions": {
        "User": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "name": {
                    "type": "string"
                },
                "last_Name": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "association": {
                    "type": "string"
                }
            }
        },
        "ApiResponse": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "integer",
                    "format": "int32"
                },
                "mensaje": {
                    "type": "string"
                }
            }
        }
    }
}